{
    "editor.minimap.enabled": false,
    "rest-client.environmentVariables": {
        "$shared": {
            "version": "v1",
            "prodToken": "foo",
            "nonProdToken": "bar",
            "protocol": "https"
        },
        // "visual.studio.debug": {
        // 	"protocol" : "http",
        // 	"host": "localhost",
        // 	"port": "53052",
        // },
        "xproduction": {
            "host": "seadserv-name",
            "protocol": "https",
            "port": "443",
            "baseurl": "query/api",
        },
        "xdevelopment": {
            "host": "localhost",
            "protocol": "http",
            "port": "53053",
            "baseurl": "api",
        }
    },
    "workbench.colorCustomizations": {
        "activityBar.activeBackground": "#b41706",
        "activityBar.background": "#b41706",
        "activityBar.foreground": "#e7e7e7",
        "activityBar.inactiveForeground": "#e7e7e799",
        "activityBarBadge.background": "#011f04",
        "activityBarBadge.foreground": "#e7e7e7",
        "commandCenter.border": "#e7e7e799",
        "sash.hoverBorder": "#b41706",
        "statusBar.background": "#831104",
        "statusBar.foreground": "#e7e7e7",
        "statusBarItem.hoverBackground": "#b41706",
        "statusBarItem.remoteBackground": "#831104",
        "statusBarItem.remoteForeground": "#e7e7e7",
        "titleBar.activeBackground": "#831104",
        "titleBar.activeForeground": "#e7e7e7",
        "titleBar.inactiveBackground": "#83110499",
        "titleBar.inactiveForeground": "#e7e7e799"
    },
    "peacock.color": "#64a2d5",
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    // "dotnet-test-explorer.enableTelemetry": false,
    // "dotnet-test-explorer.runInParallel": true,
    // "dotnet-test-explorer.showCodeLens": false,
    "[csharp]": {
        "editor.defaultFormatter": "ms-dotnettools.csharp",
        // "editor.defaultFormatter": "csharpier.csharpier-vscode"
    },
    "workbench.tree.indent": 10,
    "csharp.debug.justMyCode": true,
    "csharp.suppressDotnetRestoreNotification": true,
    "csharp.suppressBuildAssetsNotification": true,
    "csharp.suppressDotnetInstallWarning": false,
    "editor.formatOnSave": true,
    "editor.hover.delay": 600,
    "explorer.compactFolders": true,
    "csharp.semanticHighlighting.enabled": true,
    // "explorer.sortOrder": "modified",
    "explorer.sortOrderLexicographicOptions": "lower",
    "workbench.editor.highlightModifiedTabs": true,
}