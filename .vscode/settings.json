	"peacock.color": "#64a2d5",
	"files.exclude": {
		"**/.classpath": true,
		"**/.project": true,
		"**/.settings": true,
		"**/.factorypath": true
	},
	// "dotnet-test-explorer.enableTelemetry": false,
	// "dotnet-test-explorer.runInParallel": true,
	// "dotnet-test-explorer.showCodeLens": false,
	"[csharp]": {
		"editor.defaultFormatter": "ms-dotnettools.csharp",
		// "editor.defaultFormatter": "csharpier.csharpier-vscode"
	},
	"workbench.tree.indent": 10,
	"csharp.debug.justMyCode": true,
	"csharp.suppressDotnetRestoreNotification": true,
	"csharp.suppressBuildAssetsNotification": true,
	"csharp.suppressDotnetInstallWarning": false,
	"editor.formatOnSave": true,
	"editor.hover.delay": 600,
	"explorer.compactFolders": true,
	"csharp.semanticHighlighting.enabled": true,
	// "explorer.sortOrder": "modified",
	"explorer.sortOrderLexicographicOptions": "lower",
	"workbench.editor.highlightModifiedTabs": true,
}