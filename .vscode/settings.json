{
	"editor.minimap.enabled": false,
	"files.exclude": {
		"**/.classpath": true,
		"**/.project": true,
		"**/.settings": true,
		"**/.factorypath": true
	},
	"[csharp]": {
		"editor.defaultFormatter": "ms-dotnettools.csharp"
	},
	"files.watcherExclude": {
		"**/bin/**": true,
		"**/obj/**": true,
		"**/.git/objects/**": true,
		"**/.git/subtree-cache/**": true,
		"**/node_modules/*/**": true
	},
    "workbench.colorCustomizations": {
		"activityBar.activeBackground": "#2a76cf",
		"activityBar.activeBorder": "#afbcd0",
		"activityBar.background": "#2a76cf",
		"activityBar.foreground": "#e7e7e7",
		"activityBar.inactiveForeground": "#e7e7e799",
		"activityBarBadge.background": "#841b4c",
		"activityBarBadge.foreground": "#e7e7e7",
		"statusBar.background": "#215ea5",
		"statusBar.foreground": "#e7e7e7",
		"statusBarItem.hoverBackground": "#2a76cf",
		"titleBar.activeBackground": "#215ea5",
		"titleBar.activeForeground": "#e7e7e7",
		"titleBar.inactiveBackground": "#215ea599",
		"titleBar.inactiveForeground": "#e7e7e799",
		"commandCenter.border": "#e7e7e799",
		"sash.hoverBorder": "#2a76cf",
		"statusBarItem.remoteBackground": "#215ea5",
		"statusBarItem.remoteForeground": "#e7e7e7",
		"editor.background": "#000000",
		"tab.activeBackground": "#455a0b",
		"sideBar.background": "#000000",
		"gitDecoration.ignoredResourceForeground": "#7f7a7a",
		"terminal.background": "#000000",
		"outputView.background": "#000000",
		"scrollbarSlider.activeBackground": "#287545",
		"scrollbarSlider.hoverBackground": "#198e3c",
		"scrollbarSlider.background": "#515a54"
	},
	"peacock.remoteColor": "#215ea5",
	"json.schemaDownload.enable": true,
	"testing.defaultGutterClickAction": "debug"
}